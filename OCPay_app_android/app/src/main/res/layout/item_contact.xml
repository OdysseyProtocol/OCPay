    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="81dp"
        android:orientation="vertical">

        <com.ocpay.wallet.widget.SwipeMenu
            android:id="@+id/vg"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/pay_method_card_bg"
                android:layout_width="match_parent"
                android:visibility="visible"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <com.ocpay.wallet.widget.customview.CircleImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="12dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="12dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="96dp"
                        android:layout_height="16dp"
                        android:text="WALLET NAME"
                        android:textColor="#FF38525F"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="152dp"
                        android:layout_height="16dp"
                        android:text="0XDDC0222FB…75E42DERS"
                        android:textColor="#FFA4AFB4"
                        android:textSize="12sp" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="300px"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_edit"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#44ffff00"
                    android:gravity="center"
                    android:padding="20px"
                    android:text="编辑" />

                <TextView
                    android:id="@+id/tv_del"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#44ff00ff"
                    android:gravity="center"
                    android:padding="20px"
                    android:text="删除" />
            </LinearLayout>
        </com.ocpay.wallet.widget.SwipeMenu>
    </LinearLayout>
</layout>